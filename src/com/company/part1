\\ задание 1
package com.company;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {

        Scanner scan = new Scanner(System.in);
        int a = scan.nextInt();
        int b= scan.nextInt();
        int c = scan.nextInt();
        if (a+b+c==180) {
           if(a==90) {
               System.out.println("Есть прямой угол: " + a +" " + b +" " + c );
           }
           else {
               System.out.println("Нет прямого угла: "  + a +" " + b +" " + c );
           }

           }
        else {
            System.out.println("Сумма углов не равна 180 - такого треугольника не существует");
        }
    }
}

\\задание 2

package com.company;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int a = scan.nextInt();
        int b= scan.nextInt();
        int c = scan.nextInt();
        int d = scan.nextInt();
        int num1 = Math.min(a,b);
        int num2 = Math.min(c,d);
        int maxNum = Math.max(num1,num2);
        System.out.println(maxNum);

    }
}

\\задание 3

package com.company;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int x1 = scan.nextInt();
        int y1 = scan.nextInt();
        int x2 = scan.nextInt();
        int y2 = scan.nextInt();
        int x3 = scan.nextInt();
        int y3 = scan.nextInt();
        System.out.println("Координаты точек- A: " +x1 +";" +y1+ " " + "B: " +x2 +";" +y2+  " " + "C: " +x3 +";" +y3);
        if  ((x3 - x1) / (x2 - x1) == (y3 - y1) / (y2 - y1)) {
            System.out.println("Точки лежат на одной прямой");
        }
        else {
            System.out.println("Точки не лежат на одной прямой");
        }
    }
}

\\задание 4

package com.company;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int a = scan.nextInt();
        int b = scan.nextInt();
        int x = scan.nextInt();
        int y = scan.nextInt();
        int z = scan.nextInt();
        System.out.println("a = " + a + " " + "b = " + b);
        System.out.println("x = " + x + " " + "y = " + y + " " + "z = " + z);
        if (x <= a && y <= b) {
            System.out.println("Кирпич проходит");
        }
        else if (x<=a && z<=b){
            System.out.println("Кирпич проходит тоже");
        }
        else if (y<=a && z<=b){
            System.out.println("Кирпич все еще проходит");
        }
        else if (y<=a && x<=b){
            System.out.println("Кирпич все еще проходит");
        }
        else if (z<=a && x<=b){
            System.out.println("Кирпич все еще проходит");
        }
        else if (z<=a && y<=b){
            System.out.println("Кирпич все еще проходит");
        }
        else {
            System.out.println("Кирпич не пройдет");
        }
    }
}

\\задание 5

package com.company;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        double x = scan.nextDouble();
        double y;
        if (x <= 3) {
            y = Math.pow(x, 2) - 3 * x + 9;
        } else {
            y = 1 / (Math.pow(x, 3) + 6);
        }
        System.out.println(y);
    }
}

\\задание 6
package com.company;
import java.util.Scanner;

import static java.lang.Float.valueOf;

public class Main {

    public static void main(String[] args) {
      Scanner scan = new Scanner(System.in);
      int x = scan.nextInt();
      int y = scan.nextInt();
        System.out.println("A с координатами:");
        if(x>= -2 && x<=2 && y>=0 && y<=4){
            System.out.println("Точка попала в верхнюю область: true");
        }
        else if(x>= -4 && x<=4 &&  y>=-3 && y<=0 ){
            System.out.println("Точка попала в нижнюю область: true");
        }
        else{
            System.out.println("Точка не попала в область: false");
        }

    }
}







